{
  "name": "contextable",
  "version": "1.2.0",
  "description": "Simple, unopinionated and minimalist framework for creating context objects with support for unopinionated ORM, object schemas, type casting, validation and error handling and more.",
  "main": "./dist/index.js",
  "ava": {
    "files": [
      "./tests/*.js",
      "./tests/**/*.js"
    ],
    "concurrency": 4,
    "failFast": true
  },
  "scripts": {
    "clean": "rm -Rf ./dist",
    "prebuild": "npm run clean; mkdir -p ./dist",
    "build": "babel ./src --out-dir ./dist --copy-files",
    "pretest": "npm run build",
    "test": "ava",
    "prepublish": "npm run build",
    "preexample": "npm run build",
    "example": "babel-node ./example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpepermint/contextablejs.git"
  },
  "bugs": {
    "url": "https://github.com/xpepermint/contextablejs/issues"
  },
  "homepage": "https://github.com/xpepermint/contextablejs#readme",
  "keywords": [
    "orm",
    "odm",
    "map",
    "mapping",
    "context",
    "contextify",
    "contextable",
    "model",
    "modeling",
    "document",
    "class",
    "db",
    "database",
    "storage",
    "structure",
    "structuring",
    "error",
    "errors",
    "handling",
    "valid",
    "validate",
    "validation",
    "type",
    "cast"
  ],
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "devDependencies": {
    "ava": "0.16.x",
    "babel-cli": "6.18.x",
    "babel-plugin-transform-runtime": "6.15.x",
    "babel-preset-node5": "11.1.x",
    "mongodb": "2.2.x"
  },
  "dependencies": {
    "handleable": "0.3.0",
    "objectschema": "1.0.6",
    "typeable": "0.7.3"
  }
}
