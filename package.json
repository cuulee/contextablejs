{
  "name": "contextable",
  "version": "0.0.0",
  "description": "Simple, unopinionated and minimalist framework for creating ORM objects.",
  "main": "./dist/index.js",
  "ava": {
    "files": [
      "./tests/*.js",
      "./tests/**/*.js"
    ],
    "concurrency": 4,
    "failFast": true
  },
  "scripts": {
    "clean": "rm -Rf ./dist",
    "prebuild": "npm run clean; mkdir -p ./dist",
    "build": "babel ./src --out-dir ./dist --copy-files",
    "pretest": "npm run build",
    "test": "ava",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpepermint/contextablejs.git"
  },
  "bugs": {
    "url": "https://github.com/xpepermint/contextablejs/issues"
  },
  "homepage": "https://github.com/xpepermint/contextablejs#readme",
  "keywords": [
    "orm",
    "odm",
    "map",
    "mapping",
    "context",
    "contextify",
    "contextable",
    "model",
    "modeling",
    "document",
    "class",
    "db",
    "database",
    "storage",
    "structure",
    "structuring"
  ],
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "devDependencies": {
    "ava": "0.16.0"
  }
}
